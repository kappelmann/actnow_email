{
  "name": "mep_contact_frontend",
  "version": "0.0.1",
  "description": "Frontend to contact members of the European parliament (MEPs)",
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "scripts": {
    "build": "npm run lint && parcel build public/index.html --no-minify",
    "build-deploy": "npm run build && npm run deploy",
    "deploy": "rsync -avzhe ssh --progress ./dist/* cont2:/home/kevin/actnow_email/",
    "lint": "eslint src/**/*.{js,jsx,ts,tsx}",
    "prebuild": "rm -rf dist",
    "clean": "rm -rf dist",
    "start": "parcel public/index.html",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "parcel watch public/index.html",
    "preinstall": "npx npm-force-resolutions",
    "postbuild": "cp -R databases/ dist/"
  },
